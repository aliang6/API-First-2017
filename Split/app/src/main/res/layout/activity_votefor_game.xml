<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="split.split.VoteforGame">
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vote For A Game"
        android:textSize="30dp"
        android:textColor="@android:color/black"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="6dp"
        app:layout_constraintRight_toRightOf="@+id/button8"
        android:layout_marginTop="58dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="6dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="0dp"
        android:layout_height="109dp"
        android:text="Game4"
        android:background="@android:color/holo_red_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/button8"
        android:layout_marginTop="21dp"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button8" />

    <Button
        android:id="@+id/button9"
        android:layout_width="115dp"
        android:layout_height="109dp"
        android:text="Game3"
        android:background="@android:color/holo_red_light"
        android:layout_marginStart="65dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/button10"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="65dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="115dp"
        android:layout_height="0dp"
        android:text="Game2"
        android:background="@android:color/holo_red_light"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="18dp"
        android:layout_marginTop="42dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/button5"
        android:layout_marginLeft="18dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="0dp"
        android:layout_height="109dp"
        android:text="Game1"
        android:background="@android:color/holo_red_light"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/button9"
        app:layout_constraintBaseline_toBaselineOf="@+id/button8"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/button9" />

</android.support.constraint.ConstraintLayout>
